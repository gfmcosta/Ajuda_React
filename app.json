{
  "expo": {
    "name": "Ajuda+",
    "slug": "Ajuda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo_ajuda.png",
    "splash": {
      "image": "./images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goncalocosta.ajudamais"
    },
    "android": {
      "package": "com.goncalocosta.ajudamais",
      "adaptiveIcon": {
        "foregroundImage": "./images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
      
    },
    "web": {
      "favicon": "./images/favicon.png"
    }
  }
}
